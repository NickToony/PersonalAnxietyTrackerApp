<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="HoloTheme" parent="@android:style/Theme.Holo.Light">
    </style>
    
    <style name="postStarRating" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_star_drawable</item>
        <item name="android:minHeight">22dip</item>
        <item name="android:maxHeight">22dip</item>
    </style>
    
    <style name="postStarRatingBlue" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_star_drawable_blue</item>
        <item name="android:minHeight">22dip</item>
        <item name="android:maxHeight">22dip</item>
    </style>

    <color name="grey">#C0C0C0</color>
    <color name="hintTextColor">#6B6B6B</color>
    <color name="seekBarHeadingColor">#2E4FBE</color>
    <color name="backgroundColorSeekBar">#c2e4f2</color>
    <color name="colorBlack">#000000</color>
    <color name="greySeekBarColor">#d7d7d7</color>
    <color name="outlineSeekBarDarkGrey">#959595</color>
    <color name="centerCircleColour">#894325</color>
    <color name="backgroundCircleColour">#2d5c3a</color>
    <color name="dynamicCircleColour">#d7d7d7</color>
    <color name="socialBackgroundColour">#C6DEF5</color>
    <color name="social_topic">#89D1E2</color>
    <color name="social_topic_highlight">#E1ECF7</color>
    <color name="social_topic_outline">#929394</color>
    <color name="social_error">#FF0000</color>
    <color name="secondsEditTextBreathExercise">#ffffff</color>

</resources>